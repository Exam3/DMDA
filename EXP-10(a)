snowfall <-c(790,1170.8,860.1,1330.6,630.4,491.5,683.5,996.6,783.2,982,881.8,1021)
snowfall_timeseries<-ts(snowfall,start=c(2013,1),frequency=12)
plot(snowfall_timeseries)
model<-lm(snowfall_timeseries~time(snowfall_timeseries))
plot(snowfall_timeseries,main="Time series Analysis",xlab="Time",ylab="Snowfall",col="blue")
abline(model,col="red")
