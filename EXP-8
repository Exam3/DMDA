data <- c(4, 7, 8, 9, 7, 10, 5, 7, 6, 8)
mean_value <- mean(data)
print(mean_val)
median_value <- median(data)
print(median_val)
mode_value <- as.numeric(names(sort(table(data), decreasing = TRUE)[1]))
print(mode_val)

input <- mtcars[,c("am","mpg","hp")]
print(head(input))
first_result <-aov(mpg~hp*am,data=input)
print(summary(first_result))
sec_result <-aov(mpg~hp*am,data=input)
print(summary(sec_result))
print(anova(first_result,sec_result))
boxplot(anova(first_result,sec_result))
